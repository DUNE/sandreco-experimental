{
  "ufw": {
    "ufw-loglevel": "debug",
    "ufw-basepath": "/usr/local/share/sandreco/data",
    "ufw-ldpath": ["/usr/local/lib64"],
    "ufw-env": {}
  },
  "globals": {
    "sand::root_tgeomanager": {
      "geometry": "test/sand-spills.5.edep.root"
    }
  },
  "contexts": {
    "keys": 2,
    "locals": {
      "sand::edep_reader": {
        "uri": "test/sand-spills.5.edep.root"
      },
      "sand::genie_reader": {
        "uri": "test/sand-spills.5.edep.root"
      }
    }
  },
  "run": [
    {
      "sand::fake_reco::fake_reco": {},
      "reqs": {},
      "prods": {
        "output_caf": "produced_caf"
      }
    },
    {
      "sand::caf::caf_streamer": {
        "uri": "test/output.caf.root",
        "tree": "SR"
      },
      "write": ["produced_caf"]
    }
  ]
}
