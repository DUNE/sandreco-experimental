{
    "ufw" : {
      "ufw-loglevel" : "debug",
      "ufw-basepath" : "/usr/local/share/sandreco/data",
      "ufw-ldpath" : ["/usr/local/lib64"]
    },
    "globals" : {
    },
    "contexts" : {
      "keys" : 2,
      "locals" : {
      }
    },
    "run" : [
      {
        "sand::root::tree_streamer" : { 
          "uri" : "test/tracklets_file.root",
          "tree" : "tracklets_tree"
        },
        "read" : { "tracklet_map" : "sand::tracker::tracklet_map", "s_particle_infos" : "sand::tracker::s_particle_infos" }
      },
      {
        "sand::tracker::kalman_filter" : {
            "trackletinfo_placeholder": 0.0
        },
        "reqs" : {"tracklet_map" : "tracklet_map" , 
                  "s_particle_infos" : "s_particle_infos"},
        "prods" : {"tracks" : "tracks"}
      },
      {
        "sand::root::tree_streamer" : { 
          "uri" : "test/tracks.root",
          "tree" : "tracks_tree"
        },
        "write" : { "tracks" : "sand::tracker::tracks"}
      }
    ]
  }
  
