{
    "ufw" : {
      "ufw-loglevel" : "debug",
      "ufw-basepath" : "/usr/local/share/sandreco/data",
      "ufw-ldpath" : ["/usr/local/lib64"]
    },
    "globals" : {
    },
    "contexts" : {
      "keys" : 2,
      "locals" : {
      }
    },
    "run" : [
      {
        "sand::root::tree_streamer" : { 
          "uri" : "test/detresp.root",
          "tree" : "cameras"
        },
        "read" : { "foo" : "sand::grain::digi"}
      },
      {
        "sand::grain::time_slicing" : {
            "slice_times": [0.0, 20000.0]
        },
        "reqs" : {"digi" : "foo"},
        "prods" : {"images": "pippo"}
      },
      {
        "sand::root::tree_streamer" : { 
          "uri" : "test/images.root",
          "tree" : "cameras"
        },
        "write" : { "pippo" : "sand::grain::images"}
      }
    ]
  }
  
